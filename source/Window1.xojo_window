#tag WindowCode
	#tag Event
		Sub Open()
		  self.HTMLViewer1.LoadPage( TPSF.Resources().Child( "index.html" ) )
		  
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Sub sendToHTMLViewer(pic as Picture)
		  var encoded as String = EncodeBase64( pic.GetData( Picture.FormatPNG ), NoLineBreaks )
		  
		  self.HTMLViewer1.ExecuteJavaScript( "load( """ + encoded + """ );" )
		  
		End Sub
	#tag EndMethod


#tag EndWindowCode

#tag Events PushButtonSquare
	#tag Event
		Sub Action()
		  var pic as Picture = new Picture( 100, 100 )
		  
		  var g as Graphics = pic.Graphics
		  
		  g.DrawingColor = Color.Black
		  
		  g.PenSize = 3
		  
		  g.DrawRectangle( 0, 0, g.Width, g.Height )
		  
		  self.sendToHTMLViewer( pic )
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButtonRounded
	#tag Event
		Sub Action()
		  var pic as Picture = new Picture( 100, 100 )
		  
		  var g as Graphics = pic.Graphics
		  
		  g.DrawingColor = Color.Blue
		  
		  g.PenSize = 6
		  
		  g.DrawRoundRectangle( g.PenSize, g.PenSize, g.Width - ( g.PenSize * 2 ), g.Height - ( g.PenSize * 2 ), 15, 15 )
		  
		  self.sendToHTMLViewer( pic )
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButtonCircle
	#tag Event
		Sub Action()
		  var pic as Picture = new Picture( 100, 100 )
		  
		  var g as Graphics = pic.Graphics
		  
		  g.DrawingColor = Color.Red
		  
		  g.FillOval( 0, 0, g.Width, g.Height )
		  
		  self.sendToHTMLViewer( pic )
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButtonLoad
	#tag Event
		Sub Action()
		  var dlg As SelectPictureFileDialog = new SelectPictureFileDialog()
		  
		  var f as FolderItem = dlg.ShowModal()
		  
		  if f = nil then return
		  
		  var pic as Picture = Picture.Open( f )
		  
		  self.sendToHTMLViewer( pic )
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
